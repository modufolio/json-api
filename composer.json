{
    "name": "modufolio/json-api",
    "description": "JSON:API implementation for Doctrine ORM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Thiebou"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/orm": "^3.0",
        "doctrine/collections": "^2.0",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.0",
        "symfony/validator": "^7.0",
        "willdurand/negotiation": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "nyholm/psr7": "^1.8",
        "doctrine/dbal": "^4.0",
        "symfony/cache": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Modufolio\\JsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modufolio\\JsonApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
        "test:coverage": "php -d xdebug.mode=coverage ./vendor/bin/phpunit --stderr --coverage-html=tests/coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
